{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "wranglingdf"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"DesPOS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DesPOS'"
		},
		"DestinationBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DestinationBlobStorage'"
		},
		"MapDF_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MapDF'"
		},
		"POS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'POS'"
		},
		"SRC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SRC'"
		},
		"SourceBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SourceBlobStorage'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"linkedService2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService2'"
		},
		"Test_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ed2d25b5-7fa9-4673-bf90-6046518791df/resourceGroups/Installbase/providers/Microsoft.Storage/storageAccounts/refinedlayer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"DesPOS_connectionString": {
						"value": "[parameters('DesPOS_connectionString')]"
					},
					"DestinationBlobStorage_connectionString": {
						"value": "[parameters('DestinationBlobStorage_connectionString')]"
					},
					"MapDF_connectionString": {
						"value": "[parameters('MapDF_connectionString')]"
					},
					"POS_connectionString": {
						"value": "[parameters('POS_connectionString')]"
					},
					"SRC_connectionString": {
						"value": "[parameters('SRC_connectionString')]"
					},
					"SourceBlobStorage_connectionString": {
						"value": "[parameters('SourceBlobStorage_connectionString')]"
					},
					"linkedService1_connectionString": {
						"value": "[parameters('linkedService1_connectionString')]"
					},
					"linkedService2_connectionString": {
						"value": "[parameters('linkedService2_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		},
		{
			"name": "ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_1')]"
			]
		},
		{
			"name": "ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Test_properties_typeProperties_scope": {
						"value": "[parameters('Test_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_2')]"
			]
		}
	]
}